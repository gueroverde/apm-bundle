{
  "name": "gueroverde/apm-bundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Miguel Ángel Sánchez Palafox",
      "email": "pala_fox044@hotmail.com"
    }
  ],
  "prefer-stable": true,
  "require": {
    "php": "^7.1.3",
    "philkra/elastic-apm-php-agent": "^6.2",
    "symfony/yaml": "^4.1",
    "symfony/framework-bundle": "^4.0.0"
  },
  "autoload": {
    "psr-4": {
      "Gueroverde\\ApmBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gueroverde\\ApmBundle\\": "tests/"
    }
  },
  "require-dev": {
    "symfony/phpunit-bridge": "^4.0",
    "friendsofphp/php-cs-fixer": "^2.11"
  },
  "scripts": {
    "lint": "php-cs-fixer fix src --dry-run --diff",
    "lint:fix": "php-cs-fixer fix src"
  },
  "sort-packages": true
}
